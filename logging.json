{
  "version": 1,
  "disable_existing_loggers": false,
  "handlers": {
    "console": {
      "class": "logging.StreamHandler",
      "level": "DEBUG",
      "formatter": "standard"
    }
  },
  "formatters": {
    "standard": {
      "format": "[%(levelname)s] %(asctime)s | %(module)s | %(funcName)s | %(message)s"
    }
  },
  "loggers": {
    "etl.etl_utils": {
      "propagate": false,
      "handlers": ["console"]
    },
    "etl.execute_nyt_scraper": {
      "propagate": false,
      "handlers": ["console"]
    },
    "etl.rss_parser": {
      "propagate": false,
      "handlers": ["console"]
    },
    "etl": {
      "propagate": false,
      "handlers": ["console"]
    },
    "etl.parsing_utils": {
      "propagate": false,
      "handlers": ["console"]
    },
    "etl.nyt_scraper.articleAPI": {
      "propagate": false,
      "handlers": ["console"]
    }
  },
  "root": {
    "handlers": ["console"],
    "level": "DEBUG"
  }
}